{
  "name": "sfynx-project/media-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides a mediatheque to manage medias or documents",
  "keywords": ["mediatheque", "media", "document", "crop", "DDD"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxMediaBundle/contributors"
    }
  ],

  "repositories": [
    {
      "type":"git",
      "url":"https://github.com/pigroupe/DaApiClientBundle"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.8|~3.0",
    "doctrine/orm": "*",
    "doctrine/doctrine-bundle": "*",
    "sfynx-project/tool-specification-bundle": "~2.8.3",
    "sfynx-project/tool-utils-bundle": "~2.8.1",
    "sfynx-project/template-presentation-bundle": "~2.8.1",
    "sfynx-project/rest-client-bundle": "~2.8.2",
    "sfynx-project/core-bundle": "~2.9.0",
    "sfynx-project/auth-bundle": "2.x-dev"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Sfynx\\MediaBundle": "" }
  },

  "target-dir": "Sfynx/MediaBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
